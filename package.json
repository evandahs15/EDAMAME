{
  "name": "edamame",
  "version": "0.1.0",
  "main": "server/index.js",
  "repository": "https://github.com/harakeke-2019/edamame.git",
  "bugs": "https://github.com/harakeke-2019/edamame/issues",
  "author": "EDA",
  "license": "ISC",
  "scripts": {
    "start": "concurrently \"webpack\" \"node server\"",
    "dev": "concurrently \"nodemon server\" \"webpack --watch\"",
    "client": "webpack-dev-server --hot --inline",
    "server": "node server",
    "knex": "knex --knexfile ./knexfile.js",
    "updateDb": "rm -f dev.sqlite3 && yarn knex migrate:latest && yarn knex seed:run",
    "test": "jest --watchAll --testPathIgnorePatterns tests/cypress",
    "e2e": "nodemon server --ignore server/public & webpack --watch & cypress open"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "cheerio": "^1.0.0-rc.2",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "path": "^0.12.7",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-circular-progressbar": "^1.0.0",
    "react-router-dom": "^4.3.1",
    "sqlite3": "^4.0.6",
    "superagent": "^4.1.0",
    "supertest": "^3.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "cypress": "^3.1.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest": "^24.1.0",
    "nodemon": "^1.18.10",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
